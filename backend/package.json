{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node tests/integration/eav-integration.test.js",
    "test:eav": "node tests/integration/eav-integration.test.js",
    "test:eav:quick": "node tests/integration/eav-integration.test.js --quick",
    "test:eav:verbose": "node tests/integration/eav-integration.test.js --verbose",
    "test:eav:crud": "node tests/integration/eav-crud.test.js",
    "test:eav:user": "node tests/user-profile-eav.test.js",
    "test:eav:assessment": "node tests/integration/assessment-metadata-eav.test.js",
    "test:eav:facility": "node tests/integration/facility-equipment-eav.test.js",
    "test:eav:all": "node tests/integration/eav-integration.test.js && node tests/integration/eav-crud.test.js",
    "test:pre-prod": "pnpm run migrate:status && pnpm run test:eav:all",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status",
    "migrate:create": "sequelize-cli migration:generate --name",
    "migrate:baseline": "node scripts/generate-baseline-migration.js",
    "eav:setup:user": "node scripts/setup-user-profile-eav.js",
    "eav:setup:assessment": "node scripts/setup-assessment-metadata-eav.js",
    "eav:migrate:facility": "node scripts/migrate-facility-equipment-to-eav.js",
    "eav:migrate:instructor": "node scripts/migrate-instructor-awards-to-eav.js",
    "db:reset": "pnpm run migrate:undo:all && pnpm run migrate:up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.0",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.3"
  }
}
