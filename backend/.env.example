# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=uums_db
DB_USER=uums_user
DB_PASSWORD=uums_password

# Environment Mode
# Set to 'production' to disable auto-sync and enforce migrations
# Set to 'development' for development mode (default)
NODE_ENV=development

# Database Sync (Development Only)
# Set to 'false' to disable auto-sync even in development
# This is useful when you want to use migrations exclusively
ENABLE_SYNC=true

# Server Configuration
PORT=3000
JWT_SECRET=your_jwt_secret_key_here

# Other configurations...
